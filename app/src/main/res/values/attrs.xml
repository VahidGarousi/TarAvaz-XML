<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="dividerColor" format="color" />
    <declare-styleable name="TarAvazToolbar">
        <attr name="ta_title" format="string" />
        <attr name="ta_shouldShowBackBtn" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BaseItemRow">
        <attr name="bir_rowTitle" format="string" />
        <attr name="bir_viewAllText" format="string" />
        <attr name="bir_viewType" format="reference" />
        <attr name="bir_layoutManager" format="enum">
            <enum name="androidx.recyclerview.widget.LinearLayoutManager" value="0" />
            <enum name="androidx.recyclerview.widget.GridLayoutManager" value="1" />
            <enum name="androidx.recyclerview.widget.StaggeredGridLayoutManager" value="2" />
        </attr>
        <attr name="bir_orientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="springAnimationTraitStatus" format="boolean" />
    </declare-styleable>
</resources>